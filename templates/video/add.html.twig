<div class="col-md-4 p-2" id="video-{{ form.vars.name }}">
    <div class="border p-2 w-100">
		{{ form_row(form.url, {'attr': {'required': 'required'}}) }}
		{% if video is defined %}
			<iframe class="showVideo" src="{{ form.vars.data.url }}" frameborder="0" allowfullscreen></iframe>
		{% endif %}
		<button type="button" class="btn btn-sm btn-danger btn-remove mt-3" data-rel="#video-{{ form.vars.name }}">Supprimer</button>
	</div>
</div>