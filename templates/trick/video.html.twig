<div class="col-md-2 p-2" id="video-{{ form.vars.name }}">
    <div class="border p-2 w-100">
		{{ form_row(form.url) }}
		<iframe class="showVideo" src="{{ form.url.vars.data }}" frameborder="0" allowfullscreen></iframe>
		<button type="button" class="btn btn-sm btn-danger btn-remove mt-3" data-rel="#video-{{ form.vars.name }}">Supprimer</button>
	</div>
</div>